{
  "name": "generator-npm-package",
  "version": "0.0.0",
  "description": "Generator for an open-source node package.",
  "homepage": "https://github.com/patrickhulce/generator-npm-package",
  "author": {
    "name": "Patrick Hulce",
    "email": "patrick.hulce@gmail.com",
    "url": "https://patrickhulce.com/"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "generator",
    "oss",
    "travis",
    "coveralls",
    "yeoman-generator"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "lodash": "^4.15.0",
    "yeoman-generator": "^0.24.0",
    "yosay": "^1.0.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^1.2.0",
    "mocha": "^3.0.0",
    "yeoman-assert": "^2.0.0",
    "yeoman-test": "^1.0.0"
  },
  "repository": "patrickhulce/generator-npm-package",
  "scripts": {
    "test": "mocha"
  },
  "license": "MIT",
  "xo": {
    "env": [
      "browser",
      "node",
      "mocha"
    ],
    "space": 2,
    "rules": {
      "complexity": [
        "warn",
        10
      ],
      "xo/filename-case": 0,
      "no-else-return": 0,
      "max-statements-per-line": [
        "error",
        {
          "max": 2
        }
      ],
      "brace-style": [
        "error",
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "comma-dangle": [
        "error",
        "only-multiline"
      ],
      "dot-location": [
        "error",
        "object"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
